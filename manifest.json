{
  "manifest_version": 3,
  "name": "Slido Navigator",
  "version": "1.0",
  "permissions": ["storage"],
  "host_permissions": ["https://chatgpt.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}